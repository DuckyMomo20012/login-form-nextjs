generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model LOP {
  MALOP    String     @id(map: "PK__LOP__7A3DE21198309077") @db.VarChar(20)
  TENLOP   String     @db.NVarChar(100)
  MANV     String?    @db.VarChar(20)
  NHANVIEN NHANVIEN?  @relation(fields: [MANV], references: [MANV], onDelete: NoAction, onUpdate: NoAction, map: "FK__LOP__MANV__04E4BC85")
  SINHVIEN SINHVIEN[]
}

model NHANVIEN {
  MANV    String  @id(map: "PK__NHANVIEN__603F511427F69F55") @db.VarChar(20)
  HOTEN   String  @db.NVarChar(100)
  EMAIL   String? @db.VarChar(20)
  LUONG   Bytes?
  TENDN   String  @db.NVarChar(100)
  MATKHAU Bytes   @db.VarBinary(20)
  LOP     LOP[]
}

model SINHVIEN {
  MASV     String    @id(map: "PK__SINHVIEN__60228A284785EE16") @db.NVarChar(20)
  HOTEN    String    @db.NVarChar(100)
  NGAYSINH DateTime? @db.DateTime
  DIACHI   String?   @db.NVarChar(200)
  MALOP    String?   @db.VarChar(20)
  TENDN    String    @db.NVarChar(100)
  MATKHAU  Bytes     @db.VarBinary(16)
  LOP      LOP?      @relation(fields: [MALOP], references: [MALOP], onDelete: NoAction, onUpdate: NoAction, map: "FK__SINHVIEN__MALOP__03F0984C")
}
