generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Major {
  id        String    @id(map: "PK__LOP__7A3DE2112BC6731D") @map("MALOP") @db.VarChar(20)
  nameMajor String    @map("TENLOP") @db.NVarChar(100)
  idStaff   String?   @map("MANV") @db.VarChar(20)
  staffs    Staff?    @relation(fields: [idStaff], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK__LOP__MANV__534D60F1")
  courses   Course[]  @relation("HOCPHANToLOP")
  students  Student[]

  @@map("LOP")
}

model Staff {
  id          String  @id(map: "PK__NHANVIEN__603F5114CDDFC6AC") @map("MANV") @db.VarChar(20)
  name        String  @map("HOTEN") @db.NVarChar(100)
  email       String? @map("EMAIL") @db.VarChar(20)
  salary      Bytes?  @map("LUONG")
  nameCompany String  @map("TENDN") @db.NVarChar(100)
  password    Bytes   @map("MATKHAU") @db.VarBinary(20)
  keyPublic   String? @map("PUBKEY") @db.VarChar(20)
  majors      Major[]

  @@map("NHANVIEN")
}

model Student {
  id          String       @id(map: "PK__SINHVIEN__60228A2863096005") @map("MASV") @db.VarChar(20)
  name        String       @map("HOTEN") @db.NVarChar(100)
  dob         DateTime?    @map("NGAYSINH") @db.DateTime
  address     String?      @map("DIACHI") @db.NVarChar(200)
  idMajor     String?      @map("MALOP") @db.VarChar(20)
  nameCompany String       @map("TENDN") @db.NVarChar(100)
  password    Bytes        @map("MATKHAU") @db.VarBinary(16)
  major       Major?       @relation(fields: [idMajor], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK__SINHVIEN__MALOP__52593CB8")
  transcirpt  Transcript[] @relation("BANGDIEMToSINHVIEN")

  @@map("SINHVIEN")
}

model Transcript {
  idStudent String  @map("MASV") @db.VarChar(20)
  idCourse  String  @map("MAHP") @db.VarChar(20)
  mark      Bytes?  @map("DIEMTHI")
  course    Course  @relation(fields: [idCourse], references: [id], onUpdate: NoAction, map: "FK__BANGDIEM__MAHP__5629CD9C")
  student   Student @relation("BANGDIEMToSINHVIEN", fields: [idStudent], references: [id], onUpdate: NoAction, map: "FK__BANGDIEM__MASV__5535A963")

  @@id([idStudent, idCourse], map: "PK__BANGDIEM__D62178258CF874D4")
  @@map("BANGDIEM")
}

model Course {
  id          String       @id(map: "PK__HOCPHAN__603F20DA0063E3B1") @map("MAHP") @db.VarChar(20)
  nameCourse  String       @map("TENHP") @db.NVarChar(100)
  pointCredit Int?         @map("SOTC")
  idMajor     String       @map("MALOP") @db.VarChar(20)
  major       Major        @relation("HOCPHANToLOP", fields: [idMajor], references: [id], onUpdate: NoAction, map: "FK__HOCPHAN__MALOP__5441852A")
  transcript  Transcript[]

  @@map("HOCPHAN")
}
